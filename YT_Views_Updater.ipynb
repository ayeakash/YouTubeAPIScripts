        SPREADSHEET_ID = os.environ["SPREADSHEET_ID"]
        YT_API_KEY     = os.environ["YT_API_KEY"]
        SERVICE_FILE   = os.environ.get("GOOGLE_APPLICATION_CREDENTIALS")  # path set by workflow
        "\n",
        "# ‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî\n",
        "import datetime, asyncio, aiohttp, nest_asyncio, random, time\n",
        "from google.oauth2.service_account import Credentials\n",
        "from googleapiclient.discovery    import build\n",
        "import gspread, gspread.utils     as gsu\n",
        "\n",
        "nest_asyncio.apply()\n",
        "SCOPES = [\"https://www.googleapis.com/auth/spreadsheets\"]\n",
        "creds  = Credentials.from_service_account_file(SERVICE_FILE, scopes=SCOPES)\n",
        "\n",
        "# Clients\n",
        "svc  = build(\"sheets\", \"v4\", credentials=creds)      # low-level Sheets API\n",
        "gc   = gspread.authorize(creds)                      # gspread for writes\n",
        "sh   = gc.open_by_key(SPREADSHEET_ID)\n",
        "\n",
        "TODAY = datetime.date.today().isoformat()\n",
        "BATCH = 50                                           # YouTube max IDs / call\n",
        "yt_ep = \"https://www.googleapis.com/youtube/v3/videos\"\n",
        "\n",
        "# ---------- helper: exponential-back-off wrapper for any Sheets call ---\n",
        "def safe_call(req, tries=5):\n",
        "    for n in range(tries):\n",
        "        try:\n",
        "            return req.execute()\n",
        "        except Exception as e:\n",
        "            if hasattr(e, \"resp\") and e.resp.status == 429 and n < tries-1:\n",
        "                wait = (2 ** n) + random.random()\n",
        "                print(f\"‚è≥ Hit 429, retrying in {wait:.1f}s ‚Ä¶\")\n",
        "                time.sleep(wait)\n",
        "            else:\n",
        "                raise\n",
        "\n",
        "# ---------- 1 ‚Ä¢ READ EVERYTHING IN ONE SHOT ----------------------------\n",
        "sheets   = [ws for ws in sh.worksheets() if ws.title != \"Log\"]\n",
        "ranges   = [f\"'{ws.title}'!A1:ZZ\" for ws in sheets]     # whole used range/tab\n",
        "all_vals = safe_call(\n",
        "    svc.spreadsheets().values().batchGet(\n",
        "        spreadsheetId=SPREADSHEET_ID,\n",
        "        ranges=ranges,\n",
        "        majorDimension=\"ROWS\")\n",
        ")[\"valueRanges\"]\n",
        "\n",
        "# Map sheet-title ‚Üí its full 2-D values list\n",
        "sheet_data = {ws.title: vals.get(\"values\", []) for ws, vals in zip(sheets, all_vals)}\n",
        "\n",
        "# ---------- 2 ‚Ä¢ COLLECT ALL BLANK VIDEO IDS ----------------------------\n",
        "tasks = []        # [(video_id, ws_object, row_idx, date_col)]\n",
        "for ws in sheets:\n",
        "    values = sheet_data[ws.title]\n",
        "    if not values: continue\n",
        "\n",
        "    header  = values[0]\n",
        "    try:      id_col = header.index(\"Video ID\")\n",
        "    except ValueError:   # skip non-video sheets\n",
        "        continue\n",
        "\n",
        "    # ensure today's column exists (add later if missing)\n",
        "    try:\n",
        "        date_col = header.index(TODAY)\n",
        "    except ValueError:\n",
        "        date_col = len(header)\n",
        "        ws.update(range_name=gsu.rowcol_to_a1(1, date_col+1),\n",
        "                  values=[[TODAY]],\n",
        "                  value_input_option=\"RAW\")\n",
        "        for row in values:               # pad shorter rows so col lookup works\n",
        "            while len(row) <= date_col: row.append(\"\")\n",
        "\n",
        "    # walk rows 2..n\n",
        "    for r, row in enumerate(values[1:], start=2):\n",
        "        if len(row) > date_col and row[date_col]:\n",
        "            continue                      # already filled\n",
        "        vid = (row[id_col] if len(row) > id_col else \"\").strip()\n",
        "        if vid:\n",
        "            tasks.append((vid, ws, r, date_col))\n",
        "\n",
        "print(f\"üìã Need to fetch {len(tasks)} view-counts ‚Ä¶\")\n",
        "\n",
        "# ---------- 3 ‚Ä¢ ASYNC YOUTUBE FETCH IN 50-ID BATCHES -------------------\n",
        "async def fetch_chunk(session, chunk):\n",
        "    params = {\n",
        "        \"part\": \"statistics\",\n",
        "        \"id\": \",\".join(chunk),\n",
        "        \"key\": YT_API_KEY,\n",
        "        \"maxResults\": BATCH\n",
        "    }\n",
        "    async with session.get(yt_ep, params=params) as resp:\n",
        "        if resp.status != 200:                       # e.g. quota, bad key\n",
        "            txt = await resp.text()\n",
        "            print(f\"‚ö†Ô∏è  YT {resp.status} on {chunk[:3]}‚Ä¶ ‚Üí {txt[:120]} ‚Ä¶\")\n",
        "            return {}                                # mark all as Not Found\n",
        "        j = await resp.json()\n",
        "        out, missing = {}, []\n",
        "        for it in j.get(\"items\", []):\n",
        "            vc = it.get(\"statistics\", {}).get(\"viewCount\")\n",
        "            if vc is None:\n",
        "                missing.append(it[\"id\"])\n",
        "            else:\n",
        "                out[it[\"id\"]] = int(vc)\n",
        "        if missing:\n",
        "            print(f\"‚Ä¢ {len(missing)} ids in this batch had no viewCount (e.g. {missing[:3]})\")\n",
        "        return out\n",
        "\n",
        "\n",
        "async def run_fetch():\n",
        "    out = {}\n",
        "    async with aiohttp.ClientSession() as session:\n",
        "        chunks = [tasks[i:i+BATCH] for i in range(0, len(tasks), BATCH)]\n",
        "        coro   = [fetch_chunk(session, [t[0] for t in ch]) for ch in chunks]\n",
        "        for fut in asyncio.as_completed(coro):\n",
        "            out.update(await fut)\n",
        "    return out\n",
        "\n",
        "views_map = asyncio.run(run_fetch())\n",
        "print(\"‚úÖ  YouTube fetch done\")\n",
        "\n",
        "# ---------- 4 ‚Ä¢ WRITE BACK (one update per sheet) -----------------------\n",
        "from collections import defaultdict\n",
        "by_sheet = defaultdict(list)     # ws ‚Üí [(row, col, value)]\n",
        "\n",
        "for vid, ws, r, c in tasks:\n",
        "    by_sheet[ws].append( (r, c, views_map.get(vid, \"Not Found\")) )\n",
        "\n",
        "for ws, triples in by_sheet.items():\n",
        "    # sort by row for a single contiguous range write\n",
        "    triples.sort()\n",
        "    rows = [v for (_,_,v) in triples]\n",
        "    start = gsu.rowcol_to_a1(triples[0][0], triples[0][1]+1)\n",
        "    end   = gsu.rowcol_to_a1(triples[-1][0], triples[-1][1]+1)\n",
        "    ws.update(range_name=f\"{start}:{end}\",\n",
        "              values=[[v] for v in rows],\n",
        "              value_input_option=\"RAW\")\n",
        "    print(f\"‚úì  {ws.title} updated ({len(rows)} cells)\")\n",
        "\n",
        "# ---------- 5 ‚Ä¢ STAMP/CREATE LOG SHEET ---------------------------------\n",
        "try:\n",
        "    log = sh.worksheet(\"Log\")\n",
        "except gspread.WorksheetNotFound:\n",
        "    log = sh.add_worksheet(\"Log\", rows=1, cols=1)\n",
        "log.update(\"A1\", TODAY)\n",
        "\n",
        "print(\"üéâ All sheets up-to-date\")\n"
      ],
